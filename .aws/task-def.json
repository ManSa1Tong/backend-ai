{
  "family": "mansa1tong-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "mansa1tong-fastapi",
      "image": "911167894896.dkr.ecr.ap-northeast-2.amazonaws.com/mansa1tong/backend-ai:prod",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
