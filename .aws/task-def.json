{
  "family": "mansa1tong-ai-task",
  "containerDefinitions": [
      {
          "name": "mansa1tong-fastapi",
          "image": "911167894896.dkr.ecr.ap-northeast-2.amazonaws.com/mansa1tong/backend-ai:prod",
          "cpu": 0,
          "portMappings": [
              {
                  "containerPort": 8000,
                  "hostPort": 8000,
                  "protocol": "tcp"
              }
          ],
          "essential": true,
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "/ecs/mansa1tong-ai-task",
                  "awslogs-region": "ap-northeast-2",
                  "awslogs-stream-prefix": "ecs"
              }
          }
      }
  ],
  "executionRoleArn": "arn:aws:iam::911167894896:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  }
}
