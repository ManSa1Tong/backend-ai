{
  "family": "mansa1tong-ai-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::911167894896:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "mansa1tong-fastapi",
      "image": "911167894896.dkr.ecr.ap-northeast-2.amazonaws.com/mansa1tong/backend-ai:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mansa1tong-ai-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072"
}
